{
    "manifest_version": 3,
    "version": "1.1.0",
    "name": "Confluence's Buddy",
    "description": "Extens√£o para auxiliar em buscas no Sistema Atlassian Confluence",
    "author": "Eric Coutinho",
    "permissions": ["contextMenus", "storage", "management"],
    "options_page": "options.html",
    "action": {
        "default_title": "Confluence's Buddy",
        "default_popup": "popup.html",
        "options": "options.html",
        "default_icon": "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "128": "images/icon.png",
        "256": "images/icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}